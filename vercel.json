{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "BACKEND/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "FRONTEND/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "FRONTEND/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "BACKEND/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "FRONTEND/$1"
    }
  ]
}
